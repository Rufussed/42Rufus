// .vscode/launch.json
{
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Debug with GDB",
                "type": "cppdbg",
                "request": "launch",
                "program": "${workspaceFolder}/so_long_bonus",
                "args": [],
                "stopAtEntry": false,
                "cwd": "${workspaceFolder}",
                "environment": [],
                "externalConsole": false,
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ],
                "preLaunchTask": "build",
                "miDebuggerPath": "/usr/bin/gdb",
                "logging": {
                    "engineLogging": false,
                    "trace": true,
                    "traceResponse": true
                },
                "targetArchitecture": "x86_64",
                "additionalSOLibSearchPath": ""
            }
        ]
    }
    